{
  "name": "splitsies",
  "version": "1.4.0",
  "private": true,
  "scripts": {
    "android": "npm run android-emulator-ports && react-native run-android --mode localDebug",
    "ios": "react-native run-ios",
    "android:lan": "react-native run-android --mode lanDebug",
    "ios:lan": "react-native run-ios --scheme \"Splitsies LAN\"",
    "android:dev-pr": "react-native run-android --mode devprDebug",
    "android:staging": "react-native run-android --mode stagingDebug",
    "ios:dev-pr": "react-native run-ios --scheme \"Splitsies dev-pr\"",
    "build-android:dev-pr": "cd android && ENVFILE=.env.dev-pr ./gradlew assembleRelease",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "format": "prettier --write \"**/*.ts*\"",
    "android-emulator-ports": "adb reverse tcp:6001 tcp:6001 && adb reverse tcp:14624 tcp:14624 && adb reverse tcp:14623 tcp:14623 && adb reverse tcp:3000 tcp:3000 && adb reverse tcp:12948 tcp:12948",
    "clear-shared-models": "npm cache clean --force && rm -rf ./node_modules/@splitsies/shared-models && rm -rf ./package-lock.json",
    "google-services:dev-pr": "cp ./google-services/ios/dev-pr-GoogleService-Info.plist ./ios/GoogleService-Info.plist && cp ./google-services/android/dev-pr-google-services.json ./android/app/google-services.json",
    "google-services:staging": "cp ./google-services/ios/production-GoogleService-Info.plist ./ios/GoogleService-Info.plist && cp ./google-services/android/staging-google-services.json ./android/app/google-services.json",
    "google-services:production": "cp ./google-services/ios/production-GoogleService-Info.plist ./ios/GoogleService-Info.plist && cp ./google-services/android/production-google-services.json ./android/app/google-services.json"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.23.1",
    "@react-native-clipboard/clipboard": "^1.13.2",
    "@react-native-community/blur": "^4.3.2",
    "@react-native-community/datetimepicker": "^7.6.2",
    "@react-native-community/netinfo": "^9.4.1",
    "@react-native-firebase/app": "^19.2.2",
    "@react-native-firebase/messaging": "^19.2.2",
    "@react-native-picker/picker": "^2.5.1",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/drawer": "^6.6.6",
    "@react-navigation/material-top-tabs": "^6.6.5",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.14",
    "@splitsies/shared-models": "^0.0.34",
    "http2": "^3.3.7",
    "inversify": "^6.0.1",
    "inversify-inject-decorators": "^3.1.0",
    "react": "18.2.0",
    "react-native": "0.72.4",
    "react-native-bootsplash": "^5.5.3",
    "react-native-camera-kit": "^13.0.0",
    "react-native-config": "^1.5.1",
    "react-native-contacts": "^7.0.7",
    "react-native-gesture-handler": "^2.13.1",
    "react-native-google-mobile-ads": "^13.0.2",
    "react-native-image-picker": "^7.1.0",
    "react-native-keychain": "^8.1.2",
    "react-native-notifications": "^5.1.0",
    "react-native-pager-view": "^6.2.3",
    "react-native-permissions": "^4.1.4",
    "react-native-qrcode-svg": "^6.2.0",
    "react-native-reanimated": "^3.5.4",
    "react-native-safe-area-context": "^4.7.2",
    "react-native-screens": "^3.25.0",
    "react-native-svg": "^14.1.0",
    "react-native-tab-view": "^3.5.2",
    "react-native-ui-lib": "^7.8.0",
    "react-native-walkthrough-tooltip": "^1.6.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "stream": "^0.0.2"
  },
  "resolutions": {
    "react-native-permissions": "^4.0.1"
  },
  "overrides": {
    "react-native-qrcode-svg": {
      "react-native-svg": "$react-native-svg"
    },
    "react-native-qrcode-scanner": {
      "react-native-permissions": "^4.0.1"
    }
  },
  "prettier": "@splitsies/formatting",
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/plugin-proposal-decorators": "^7.23.0",
    "@babel/plugin-syntax-decorators": "^7.22.10",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.23.1",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.11",
    "@splitsies/formatting": "^1.0.0",
    "@tsconfig/react-native": "^3.0.0",
    "@types/react": "^18.0.24",
    "@types/react-native-vector-icons": "^6.4.15",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.76.8",
    "prettier": "^2.4.1",
    "react-native-svg-transformer": "^1.3.0",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "engines": {
    "node": ">=16"
  }
}
